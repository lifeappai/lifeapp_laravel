container_commands:
    01-permission:
        command: "chmod 777 -R storage/ bootstrap/"
    02-config_clear:
        command: "php artisan config:clear"
    03-view_clear:
        command: "php artisan view:clear"
    04-route_cache:
        command: "php artisan route:cache"
    05-view_cache:
        command: "php artisan view:cache"
    06-migrate:
        command: "php artisan migrate --force"
        leader_only: true
    07-create_folder:
        command: "mkdir -p /var/log/application_log"
    08-permission_log:
        command: "chmod 777 -R /var/log/"
    09-log_link:
        command: "php artisan storage:link"
    10-passport:
        command: "php artisan passport:install"
